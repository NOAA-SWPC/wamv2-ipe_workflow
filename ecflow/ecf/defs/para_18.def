extern /para/primary/18/gfs/v16.2/gdas/atmos/obsproc/dump/jgdas_atmos_dump_post
extern /para/primary/18/gfs/v16.2/gfs/atmos/obsproc/dump/jgfs_atmos_dump_post

suite para
  family primary
    family 18
      family wfs
        defstatus complete
        edit PROJ 'WFS'
        edit PACKAGEHOME '/lfs/h1/ops/%ENVIR%/packages/wfs.%wfs_ver%'

        family v1.1
          family wdas
            task jwdas_prep_noda
              trigger /para/primary/18/gfs/v16.2/gdas/atmos/obsproc/dump/jgdas_atmos_dump_post == complete
            task jwdas_analysis_noda
              trigger jwdas_prep_noda == complete
              event 1 release_fcst
            task jwdas_forecast
              trigger jwdas_analysis_noda:release_fcst or jwdas_analysis_high == complete
          endfamily

          family wfs
            task jwfs_prep_noda
              trigger /para/primary/18/gfs/v16.2/gfs/atmos/obsproc/dump/jgfs_atmos_dump_post == complete
            task jwfs_analysis_noda
              trigger jwfs_prep_noda eq complete
              event 1 release_fcst
            task jwfs_forecast_high
              trigger jwfs_analysis:release_fcst or jwfs_analysis == complete
          endfamily
        endfamily

      endfamily
    endfamily
  endfamily
endsuite
